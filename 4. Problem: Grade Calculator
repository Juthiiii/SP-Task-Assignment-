#include<stdio.h>
int main()
{
   int score;

   printf("Enter score( 0-100 ): ");
   scanf("%d", &score);

   if(score == 100) {
       score -= 1;
   }

   switch( score / 10 )
   {

   case 9:
     printf("Grade: A");
     break;

   case 8:
     printf("Grade: B");
     break;

   case 7:
     printf("Grade: C");
     break;

   case 6:
     printf("Grade: D");
     break;

   case 5:
     printf("Grade: E");
     break;

   case 4:
   case 3:
   case 2:
   case 1:
   case 0:
     printf("Grade: F");
     break;

   default:
     printf("Invalid input");
     break;

   }

   return 0;
}
